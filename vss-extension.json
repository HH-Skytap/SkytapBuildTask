{
    "manifestVersion": 1,
    "id": "SkytapEnvironmentDeployment",
    "publisher": "JasonMarston",
    "version": "1.0.0",
    "name": "Skytap Environment Deployment",
    "description": "Skytap Environment Deployment",
    "public": false,
    "categories": [
        "Build and release"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "skytap.png"
    },
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ],
    "files": [
        {
            "path": "buildAndReleaseTask"
        }
    ]
}